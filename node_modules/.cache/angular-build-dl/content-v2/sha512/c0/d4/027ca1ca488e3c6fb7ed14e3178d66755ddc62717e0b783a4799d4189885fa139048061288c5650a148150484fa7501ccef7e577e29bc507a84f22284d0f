          this.route.navigate(['/submit']);
        }
      }, {
        key: "redirectToLogin",
        value: function redirectToLogin() {
          this.authService.doGoogleAuth().then(function (res) {
            location.reload();
          }, function (err) {
            return console.log(err);
          });
        } //redirectToLogin()   { this.route.navigate(['/login']);    }

      }, {
        key: "redirectToUserInfo",
        value: function redirectToUserInfo(email) {
          this.route.navigate(['/userinfo', email]);
        }
      }, {
        key: "redirectToContributionInfo",
        value: function redirectToContributionInfo(id) {
          this.route.navigate(['/contributioninfo', id]);
        }
      }]);

      return NewestComponent;
    }();

