        this.firebaseUserService = firebaseUserService;
        this.authService = authService;
        this.activatedRoute = activatedRoute;
        this.loggedUser = new _core_user_model__WEBPACK_IMPORTED_MODULE_2__["FirebaseUserModel"]();
        this.requestedUser = new _core_user_model__WEBPACK_IMPORTED_MODULE_2__["FirebaseUserModel"]();
        this.logged = false;
        this.isSubmitted = false;
        this.sameUser = false;
      }

      _createClass(UserInfoComponent, [{
        key: "ngOnInit",
        value: function ngOnInit() {
          var _this9 = this;

          this.requestedUser.email = this.activatedRoute.snapshot.paramMap.get("email");
          this.firebaseUserService.getCurrentUser().then(function (user) {
            // user logged
            _this9.logged = true;
            _this9.loggedUser.name = user.displayName;
            _this9.loggedUser.email = user.email;
            _this9.loggedUser.provider = user.providerData[0].providerId;
            user.getIdToken(true).then(function (idToken) {
              _this9.loggedUser.google_token = idToken;
            }); // same user

            if (_this9.requestedUser.email == _this9.loggedUser.email) _this9.sameUser = true;
          }, function (err) {
            _this9.logged = false;
          }); // user shown

          this.userService.getByEmail(this.requestedUser.email).subscribe(function (response) {
            console.log("requested user info:", response);
            _this9.requestedUser = response;
          });
          this.userForm = this.formBuilder.group({
            about: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_1__["Validators"].required]]
          });
        }
      }, {
        key: "onSubmit",
        value: function onSubmit() {
          this.isSubmitted = true;
          this.userService.updateUser(this.requestedUser.id, this.createUserForm().about).subscribe(function (response) {
            console.log(response);
          });
        }
      }, {
        key: "createUserForm",
        value: function createUserForm() {
          var updatedUser = {
            name: this.requestedUser.name,
            email: this.requestedUser.email,
            provider: this.requestedUser.provider,
            id: this.requestedUser.id,
            created_at: this.requestedUser.created_at,
            updated_at: new Date(),
            google_token: this.requestedUser.google_token,
            google_refresh_token: this.requestedUser.google_refresh_token,
            about: this.userForm.value,
            api_key: this.requestedUser.api_key,
            karma: this.requestedUser.karma
          };
          return updatedUser;
        }
      }, {
        key: "logout",
        value: function logout() {
          this.authService.doLogout();
          location.reload();
        }
      }, {
        key: "redirectToNews",
